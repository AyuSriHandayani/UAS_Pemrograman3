<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.my.diary"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.my.diary.MainScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.my.diary.TambahData"
            android:label="@string/title_activity_tambah_data" >
        </activity>
        <activity
            android:name="com.my.diary.AllDiary"
            android:label="@string/title_activity_all_diary" >
        </activity>
        <activity
            android:name="com.my.diary.ListItem"
            android:label="@string/title_activity_list_item" >
        </activity>
    </application>

</manifest>
